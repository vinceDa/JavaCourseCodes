<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:aop="http://www.springframework.org/schema/aop"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                        http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
                        http://www.springframework.org/schema/context
                        http://www.springframework.org/schema/context/spring-context-3.2.xsd http://www.springframework.org/schema/aop https://www.springframework.org/schema/aop/spring-aop.xsd">
    
    <bean id="student01"
          class="homework.Student">
        <property name="name" value="s01" />
        <property name="age" value="5" />
        <property name="sex" value="1" />
        <property name="email" value="vvv@qq.com" />
        <property name="address" value="澳门赌场" />
    </bean>

    <bean id="student02"
          class="homework.Student">
        <property name="name" value="s02" />
        <property name="age" value="50" />
        <property name="sex" value="0" />
        <property name="email" value="iii@163.com" />
        <property name="address" value="鸟巢" />
    </bean>

    
</beans>